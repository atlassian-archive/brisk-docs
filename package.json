        "@babel/preset-typescript": "^7.3.3",
        "@emotion/core": "^10.0.9",
        "@emotion/styled": "^10.0.9",
        "@mdx-js/loader": "^0.17.5",
        "@mdx-js/mdx": "^0.17.5",
        "@mdx-js/tag": "^0.17.5",
        "@zeit/next-css": "^1.0.1",
        "@zeit/next-mdx": "^1.2.0",
        "babel-loader": "^8.0.5",
        "babel-plugin-transform-dynamic-import": "^2.1.0",
        "babel-plugin-transform-flow-strip-types": "^6.22.0",
        "bolt": "^0.22.6",
        "command-line-args": "^5.0.2",
