        "eslint-plugin-jsx-a11y": "^6.2.1",
        "eslint-plugin-react": "^7.12.4",
        "extracted-loader": "^1.0.7",
        "file-loader": "^2.0.0",
        "jest": "^24.1.0",
        "json-loader": "^0.5.7",
        "nodemon": "^1.12.1",
