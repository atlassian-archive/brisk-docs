{
    "name": "brisk-docs",
    "version": "1.0.0",
    "description": "The brisk-docs website generator",
    "main": "index.js",
    "license": "MIT",
    "private": true,
    "scripts": {
        "test": "jest",
        "cypress": "node_modules/.bin/cypress run",
        "build": "bolt ws run build",
        "dev:website": "bolt w @brisk-docs/website run dev",
        "build:website": "bolt w @brisk-docs/website run build:site",
        "build:docs": "NODE_ENV=production node packages/website/bin/index.js build",
        "clean": "yarn delete:modules && yarn delete:caches && delete:dist",
        "delete:caches": "rm -rf packages/website/.next && rm -rf packages/website/pages/packages && rm -rf packages/website/pages/docs",
        "delete:dist": "bolt ws exec --parallel -- rm -rf dist",
        "delete:modules": "bolt ws exec --parallel -- rm -rf node_modules && rm -rf node_modules",
        "docs": "node packages/website/bin/index.js dev",
        "start": "bolt w @brisk-docs/website run start",
        "prettier": "prettier --write \"**/*.js\"",
        "lint:prettier": "prettier --list-different \"**/*.js\"",
        "lint:eslint": "yarn eslint .",
        "lint": "yarn lint:eslint && yarn lint:prettier",
        "test:logs": "bolt w @brisk-docs/react-changelogs run jest",
        "changeset": "build-releases changeset",
        "version-packages": "build-releases version",
        "publish-changed": "yarn build && build-releases publish --public"
    },
    "bolt": {
        "version": "^0.22.1",
        "workspaces": [
            "packages/*"
        ]
    },
    "dependencies": {
        "@atlaskit/avatar": "^14.1.8",
        "@atlaskit/banner": "^8.0.1",
        "@atlaskit/button": "^8.2.4",
        "@atlaskit/css-reset": "^3.0.5",
        "@atlaskit/docs": "^6.0.2",
        "@atlaskit/drawer": "^2.7.1",
        "@atlaskit/dropdown-menu": "^6.1.26",
        "@atlaskit/dynamic-table": "^10.0.23",
        "@atlaskit/icon": "^16.0.1",
        "@atlaskit/logo": "^9.1.0",
        "@atlaskit/navigation-next": "^4.1.2",
        "@atlaskit/pagination": "^10.0.1",
        "@atlaskit/quick-search": "^5.2.5",
        "@atlaskit/section-message": "^1.0.17",
        "@atlaskit/table-tree": "^5.0.7",
        "@atlaskit/theme": "^6.0.0",
        "@atlaskit/tree": "^4.1.11",
        "@atlaskit/width-detector": "^0.3.1",
        "@babel/cli": "^7.2.3",
        "@babel/core": "^7.3.4",
        "@babel/plugin-proposal-class-properties": "^7.3.4",
        "@babel/plugin-proposal-object-rest-spread": "^7.3.4",
