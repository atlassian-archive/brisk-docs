        "lodash.flatmap": "^4.5.0",
        "lodash.identity": "^3.0.0",
        "lodash.snakecase": "^4.1.1",
        "next": "^7.0.2",
        "outdent": "^0.7.0",
        "pretty-proptypes": "^0.6.0",
        "prismjs": "^1.15.0",
        "prop-types": "^15.6.2",
        "raw-loader": "^1.0.0",
        "react": "^16.8.4",
