        "@types/react": "^16.4.18",
        "@types/semver": "^5.5.0",
        "@typescript-eslint/parser": "^1.4.2",
        "babel-eslint": "^10.0.1",
        "babel-plugin-emotion": "^10.0.9",
        "babel-plugin-styled-components": "^1.10.0",
        "css-loader": "^1.0.0",
